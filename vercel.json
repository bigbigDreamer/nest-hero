{
  "version": 2,
  "name": "nest-hero",
  "builds": [
    {
      "src": "**",
      "use": "@vercel/node",
      "config": {
        "include": [
          "config",
          "public"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": [
        "POST",
        "GET",
        "PUT",
        "DELETE"
      ]
    }
  ]
}
