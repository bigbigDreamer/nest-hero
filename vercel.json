{
  "version": 2,
  "name": "nest-hero",
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": [
        "POST",
        "GET",
        "PUT",
        "DELETE"
      ]
    },
    {
      "src": "/api",
      "dest": "/api"
    }
  ]
}
